{
  "name": "pwa-builder-blog",
  "version": "1.0.0",
  "description": "PWA Builder Blogging site",
  "license": "MIT",
  "_moduleAliases": {
    "@config": ".config.js"
  },
  "scripts": {
    "clean": "rimraf '{dist,.picture.cache}'",
    "dev:webpack": "webpack --mode development",
    "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy",
    "dev": "npm-run-all clean --parallel dev:* --print-label",
    "build:webpack": "webpack --mode production",
    "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "build": "run-s clean build:* --print-label",
    "watch:webpack": "webpack --mode development --watch",
    "watch:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "watch": "npm-run-all clean --parallel watch:* --print-label",
    "start": "npm run watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "cssnano": "^4.1.10",
    "deasync": "^0.1.21",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "jsonfile": "^6.1.0",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-imsize": "^2.0.1",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.1",
    "tailwindcss": "^2.0.3",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
